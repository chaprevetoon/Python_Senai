import tkinter as tk



root  = tk.Tk()
root.title('Isso é um titulo')
root.geometry('300x300')


texto  =  tk.Label(root, text = 'Isso é um texto')
texto.pack()


btn = tk.Button(root,text = 'Clique aqui')
btn.pack()


entrada = tk.Entry(root)
entrada.pack()



root.mainloop()